{
  "name": "electron-react-boiler",
  "version": "0.1.0",
  "description": "Electron + CRA Boilerplate",
  "author": "mmfallacy",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@use-it/interval": "^0.1.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "framer-motion": "1.11.1",
    "moment": "^2.27.0",
    "node-sass": "^7.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-indiana-drag-scroll": "^1.6.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.4.1",
    "react-use-gesture": "^7.0.15",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "react-test": "craco test",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\" ",
    "rebuild": "electron-rebuild -f"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.11.0",
    "wait-on": "^5.0.1"
  },
  "homepage": "./",
  "build": {
    "appId": "your.app.id",
    "win": {
      "target": "NSIS",
      "icon": "public/favicon.png"
    },
    "publish": null
  }
}
